<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<DialogPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="100.0" xmlns="http://javafx.com/javafx/8.0.111"
            fx:controller="org.jabref.gui.filelist.LinkedFileEditDialogController">
   <content>
       <VBox maxWidth="1.7976931348623157E308">
           <HBox prefHeight="100.0" spacing="10.0">
               <padding>
                   <Insets top="10.0"/>
               </padding>
               <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="95.0" text="%Link">
                   <padding>
                       <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                   </padding>
               </Label>
               <TextField fx:id="link" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity"/>
               <Button minWidth="-Infinity" mnemonicParsing="false" onAction="#openBrowseDialog" text="%Browse"/>
           </HBox>
           <HBox layoutX="21.0" layoutY="21.0" prefHeight="100.0" spacing="10.0">
               <Label minWidth="95.0" text="Description">
                   <HBox.margin>
                       <Insets/>
                   </HBox.margin>
                   <padding>
                       <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                   </padding>
               </Label>
               <TextField fx:id="description" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
                          minWidth="-Infinity"/>
           </HBox>
           <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
               <Label minWidth="95.0" text="%File type">
                   <padding>
                       <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                   </padding>
               </Label>
               <ComboBox fx:id="fileType"/>
           </HBox>
       </VBox>
   </content>
</DialogPane>
